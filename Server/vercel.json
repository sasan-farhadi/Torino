{
    "version": 2,
    "builds": [
      {
        "src": "server/app.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/server/app.js"
      },
      {
        "src": "/public/(.*)",
        "dest": "/server/public/$1"
      },
      {
        "src": "/swagger",
        "dest": "/server/swagger.json"
      }
    ]
  }
  