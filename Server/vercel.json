{
    "version": 2,
    "builds": [
        {
            "src": "./app.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/app.js"
        },
        {
            "src": "/public/images/(.*)",
            "dest": "/public/images/$1"
        },
        {
            "src": "/api/auth/(.*)",
            "dest": "/server/routes/auth.js"
        }
    ]
}