{
    "version": 2,
    "builds": [
      {
        "src": "server/app.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/static/(.*)",
        "dest": "/server/public/$1"
      },
      {
        "src": "/api-docs",
        "dest": "/server/api-docs"
      },
      {
        "src": "/(.*)",
        "dest": "/server/app.js"
      }
    ]
  }
  