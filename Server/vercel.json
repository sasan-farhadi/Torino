{
    "version": 2,
    "builds": [
      {
        "src": "server/app.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/server/$1"
      },
      {
        "src": "/public/(.*)",
        "dest": "/server/public/$1"
      },
      {
        "src": "/swagger",
        "dest": "/server/swagger.json"
      }
    ],
    "env": {
      "NODE_ENV": "production"
    }
  }
  